{
  "name": "aurelia-hacker-news",
  "version": "1.0.0",
  "description": "Hacker News clone built with Aurelia",
  "homepage": "https://github.com/michaelbull/aurelia-hacker-news#readme",
  "private": true,
  "main": "src/main.ts",
  "license": "ISC",
  "author": {
    "name": "Michael Bull",
    "url": "https://www.michael-bull.com"
  },
  "keywords": [
    "aurelia",
    "typescript",
    "webpack",
    "hacker",
    "news",
    "clone"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaelbull/aurelia-hacker-news.git"
  },
  "bugs": {
    "url": "https://github.com/michaelbull/aurelia-hacker-news/issues"
  },
  "scripts": {
    "start": "webpack-dev-server --env=development --inline --hot --open",
    "build": "webpack --env=production",
    "deploy": "npm run clean && npm run build && push-dir --dir=dist --branch=gh-pages",
    "clean": "rimraf dist && rimraf reports"
  },
  "dependencies": {
    "aurelia-bootstrapper": "2.2.0",
    "aurelia-http-client": "1.2.1",
    "firebase": "4.10.1",
    "normalize.css": "8.0.0",
    "nprogress": "0.2.0",
    "rimraf": "2.6.2",
    "timeago.js": "3.0.2"
  },
  "devDependencies": {
    "@types/nprogress": "0.0.29",
    "@types/webpack": "3.8.8",
    "@types/webpack-env": "1.13.5",
    "aurelia-webpack-plugin": "2.0.0-rc.5",
    "awesome-typescript-loader": "3.5.0",
    "breakpoint-sass": "2.7.1",
    "css-loader": "0.28.10",
    "css-mqpacker": "6.0.2",
    "cssnano": "3.10.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.9",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "2.30.1",
    "node-sass": "4.7.2",
    "postcss-loader": "2.1.0",
    "push-dir": "0.4.1",
    "resolve-url-loader": "2.2.1",
    "sass-loader": "6.0.6",
    "style-loader": "0.20.2",
    "tslint": "5.9.1",
    "tslint-loader": "3.5.3",
    "typescript": "2.7.2",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.11.1"
  }
}
