{
  "name": "aurelia-hacker-news",
  "version": "1.0.0",
  "description": "Hacker News clone built with Aurelia",
  "homepage": "https://github.com/michaelbull/aurelia-hacker-news#readme",
  "private": true,
  "main": "src/main.ts",
  "license": "ISC",
  "author": {
    "name": "Michael Bull",
    "url": "https://www.michael-bull.com"
  },
  "keywords": [
    "aurelia",
    "typescript",
    "webpack",
    "hacker",
    "news",
    "clone"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaelbull/aurelia-hacker-news.git"
  },
  "bugs": {
    "url": "https://github.com/michaelbull/aurelia-hacker-news/issues"
  },
  "scripts": {
    "start": "webpack-dev-server --inline --hot --open",
    "build": "cross-env NODE_ENV=production webpack --hide-modules",
    "deploy": "npm run clean && npm run build && push-dir --dir=dist --branch=gh-pages",
    "clean": "rm -rf dist/* && rm -rf reports/*"
  },
  "dependencies": {
    "aurelia-bootstrapper": "2.1.1",
    "aurelia-event-aggregator": "1.0.1",
    "aurelia-framework": "1.1.2",
    "aurelia-history-browser": "1.0.0",
    "aurelia-http-client": "1.1.1",
    "aurelia-logging-console": "1.0.0",
    "aurelia-templating-binding": "1.3.0",
    "aurelia-templating-resources": "1.4.0",
    "aurelia-templating-router": "1.1.0",
    "firebase": "4.1.2",
    "nprogress": "0.2.0",
    "timeago.js": "3.0.1"
  },
  "devDependencies": {
    "@types/nprogress": "0.0.29",
    "@types/webpack": "2.2.15",
    "@types/webpack-env": "1.13.0",
    "aurelia-webpack-plugin": "2.0.0-rc.2",
    "awesome-typescript-loader": "3.1.3",
    "cross-env": "5.0.1",
    "css-loader": "0.28.4",
    "css-mqpacker": "6.0.1",
    "cssnano": "3.10.0",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "handlebars": "4.0.10",
    "handlebars-loader": "1.5.0",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "node-sass": "4.5.3",
    "normalize.css": "7.0.0",
    "postcss-loader": "2.0.5",
    "push-dir": "0.4.1",
    "resolve-url-loader": "2.0.2",
    "sass-lint": "1.10.2",
    "sass-loader": "6.0.5",
    "sasslint-webpack-plugin": "1.0.4",
    "style-loader": "0.18.2",
    "tslint": "5.2.0",
    "tslint-loader": "3.5.3",
    "typescript": "2.3.4",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.4.5"
  }
}
