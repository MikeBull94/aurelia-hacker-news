{
  "name": "aurelia-hacker-news",
  "version": "1.0.0",
  "description": "HackerNews clone written using Aurelia",
  "private": true,
  "scripts": {
    "start": "npm run server:dev",
    "server:dev": "webpack-dev-server --inline --hot --open",
    "server:prod": "ws --port 8080 --spa index.html --rewrite '/api/* -> http://djangoservice:9997/$1'",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "test": "cross-env NODE_ENV=test karma start test/karma.conf.js --singleRun true ",
    "test:watch": "cross-env NODE_ENV=test karma start test/karma.conf.js --singleRun false --reporters spec",
    "e2e": "npm run e2e:install && npm run e2e:run",
    "e2e:install": "cross-env ./node_modules/.bin/webdriver-manager update",
    "e2e:run": "protractor test/protractor.conf.js",
    "docs": "typedoc --out docs/ --mode file --includeDeclarations --target es6 --name aurelia-hacker-news --readme none --moduleResolution node src/",
    "clean": "rm -rf dist/* && rm -rf reports/* && rm -rf docs/*"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/MikeBull94/aurelia-hacker-news.git"
  },
  "dependencies": {
    "aurelia-bootstrapper-webpack": "1.0.0",
    "aurelia-event-aggregator": "1.0.0",
    "aurelia-framework": "1.0.7",
    "aurelia-history-browser": "1.0.0",
    "aurelia-http-client": "1.0.3",
    "aurelia-logging-console": "1.0.0",
    "aurelia-templating-binding": "1.0.0",
    "aurelia-templating-resources": "1.1.1",
    "aurelia-templating-router": "1.0.0"
  },
  "devDependencies": {
    "@types/core-js": "0.9.34",
    "@types/jasmine": "2.5.38",
    "@types/node": "6.0.51",
    "@types/selenium-webdriver": "2.53.35",
    "aurelia-bootstrapper": "1.0.1",
    "aurelia-protractor-plugin": "1.0.1",
    "aurelia-testing": "1.0.0-beta.2.0.0",
    "aurelia-webpack-plugin": "1.0.0-beta.4.0.1",
    "awesome-typescript-loader": "2.2.4",
    "cross-env": "3.1.3",
    "css-loader": "0.26.0",
    "cssnano": "3.8.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "firebase": "3.6.1",
    "html-loader": "0.4.4",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-core": "2.5.2",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-htmlfile-reporter": "0.3.4",
    "karma-jasmine": "1.0.2",
    "karma-remap-coverage": "0.1.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-sourcemap-writer": "0.1.2",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "1.8.0",
    "local-web-server": "1.2.6",
    "moment": "2.17.0",
    "node-sass": "3.13.0",
    "normalize.css": "5.0.0",
    "optimize-css-assets-webpack-plugin": "1.3.0",
    "protractor": "4.0.11",
    "protractor-jasmine2-html-reporter": "0.0.6",
    "resolve-url-loader": "1.6.0",
    "sass-lint": "1.9.1",
    "sass-loader": "4.0.2",
    "sasslint-webpack-plugin": "1.0.4",
    "style-loader": "0.13.1",
    "ts-node": "1.7.0",
    "tslint": "3.14.0",
    "tslint-loader": "2.1.5",
    "typedoc": "0.5.1",
    "typescript": "2.0.10",
    "webpack": "1.13.3",
    "webpack-dev-server": "1.16.2"
  }
}
