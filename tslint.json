{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "max-line-length": [
      false
    ],
    "member-access": [
      true,
      "no-public"
    ],
    "member-ordering": [
      true,
      {
        "order": [
          "static-field",
          "static-method",
          "instance-field",
          "constructor",
          "instance-method"
        ]
      }
    ],
    "no-console": false,
    "no-string-literal": false,
    "object-literal-key-quotes": [
      true,
      "consistent"
    ],
    "object-literal-sort-keys": false,
    "only-arrow-functions": false,
    "prefer-const": false,
    "prefer-for-of": false,
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": false,
    "space-before-function-paren": [
      true,
      {
        "anonymous": "always"
      }
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "typedef": [
      true,
      "call-signature",
      "arrow-call-signature",
      "parameter",
      "arrow-parameter",
      "property-declaration",
      "member-variable-declaration",
      "object-destructuring",
      "array-destructuring"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ]
  },
  "rulesDirectory": []
}
